---
import CommonSubCategory from "@components/commoncategory/CommonSubCategory.astro";

// Props passed from getStaticPaths/getStaticProps
const currentPath = Astro.url.pathname;
const { catslug } = Astro.params;
const lang = currentPath.startsWith("/en") ? "en" : "ar";
---

<CommonSubCategory {catslug} {lang} />
